{
	"easycom": {
			"autoscan": true,
			// 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
			"custom": {
				"^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
				"^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
		    "^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
			}
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path" : "pages/like/like",
			"style" : 
			{
				"navigationBarTitleText" : "喜欢"
			}
		},
		{
			"path" : "pages/my/my",
			"style" : 
			{
				"navigationBarTitleText" : "我的"
			}
		}
	],
	"tabBar": {
		"color": "#7a7e83",
		"selectedColor": "#2867ce",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath":"pages/index/index",
				"iconPath":"/static/tabbar/index1.png",
				"selectedIconPath": "/static/tabbar/index.png",
				"text": "首页"
			},
			{
				"pagePath":"pages/like/like",
				"iconPath":"/static/tabbar/like1.png",
				"selectedIconPath": "/static/tabbar/like.png",
				"text": "喜欢"
			},
			{
				"pagePath":"pages/my/my",
				"iconPath":"/static/tabbar/my1.png",
				"selectedIconPath": "/static/tabbar/my.png",
				"text": "我的"
			}
		],
		
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
